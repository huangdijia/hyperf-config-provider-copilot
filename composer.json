{
    "name": "huangdijia/hyperf-config-provider-copilot",
    "description": "This is my package hyperf-config-provider-copilot",
    "keywords": [
        "huangdijia",
        "hyperf-config-provider-copilot"
    ],
    "homepage": "https://github.com/huangdijia/hyperf-config-provider-copilot",
    "license": "MIT",
    "authors": [
        {
            "name": "Deeka Wong",
            "email": "huangdijia@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Huangdijia\\HyperfConfigProviderCopilot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Huangdijia\\HyperfConfigProviderCopilot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "cs-fix": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
